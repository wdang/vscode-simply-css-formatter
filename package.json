{
  "name": "vscode-clean-css-formatter",
  "displayName": "Clean CSS Formatter",
  "description": "CSS formatter using Clean CSS",
  "version": "0.0.1",
  "publisher": "williamdang",
  "icon": "icon.png",
  "license": "MIT",
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wdang/vscode-clean-css-formatter"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "keywords": [
    "multi-root ready"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:css"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/clean-css": "^4.2.1",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.43.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "clean-css": "^4.2.3",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "clean-css": "^4.2.3"
  },
  "contributes": {
    "configuration": {
      "title": "Simply CSS Formatter",
      "properties": {
        "simplycss.useCodeSnippetsOnMethodSuggest": {
          "type": "boolean",
          "default": false,
          "description": "Complete functions with their parameter signature."
        },
        "simplycss.tsdk": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "Specifies the folder path containing the tsserver and lib*.d.ts files to use."
        }
      }
    }
  }
}
